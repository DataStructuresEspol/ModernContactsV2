<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dsa.contacts.controllers.ChoiceController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Button mnemonicParsing="false" onMouseClicked="#addPerson" text="Persona" />
      <Button mnemonicParsing="false" onMouseClicked="#AddCompany" text="Empresa" />
      <Button mnemonicParsing="false" onMouseClicked="#retroceder" text="retroceder" />
   </children>
</VBox>
